{
  "api": {
    "buzzinapi": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "function": {
    "getActiveUsersAtLocation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "listNearbyLocations": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "performSwipe": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  }
}
